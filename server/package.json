{
  "name": "server",
  "version": "1.0.0",
  "module": "index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "ingest": "tsx scripts/IngestStudentsFromExcel.ts",
    "migrate:shared-contacts": "tsx scripts/MigrateToSharedContacts.ts",
    "create-superadmin": "tsx scripts/CreateSuperAdmin.ts",
    "generate-fees": "tsx scripts/GenerateMissingFeeRecords.ts",
    "generate-fees:dry-run": "tsx scripts/GenerateMissingFeeRecords.ts --dry-run",
    "cleanup-fees": "tsx scripts/CleanupExcessFeeRecords.ts",
    "cleanup-fees:dry-run": "tsx scripts/CleanupExcessFeeRecords.ts --dry-run",
    "fix-fees": "tsx scripts/FixFeeRecordsInDB.ts",
    "fix-fees:dry-run": "tsx scripts/FixFeeRecordsInDB.ts --dry-run",
    "fix-fees:all": "tsx scripts/FixFeeRecordsInDB.ts --include-inactive",
    "fix-batches": "tsx scripts/FixBatchesAndStudents.ts",
    "fix-batches:dry-run": "tsx scripts/FixBatchesAndStudents.ts --dry-run",
    "verify-from-excel": "tsx scripts/VerifyAndFixFromExcel.ts",
    "verify-from-excel:dry-run": "tsx scripts/VerifyAndFixFromExcel.ts --dry-run",
    "fix-discrepancies": "tsx scripts/FixPaymentDiscrepancies.ts --apply",
    "fix-discrepancies:dry-run": "tsx scripts/FixPaymentDiscrepancies.ts",
    "analyze-status": "tsx scripts/AnalyzeFeeAndStudentStatus.ts",
    "fix-payment-amounts": "tsx scripts/FixPaymentDateWithZeroAmount.ts --apply",
    "fix-payment-amounts:dry-run": "tsx scripts/FixPaymentDateWithZeroAmount.ts"
  },
  "dependencies": {
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@types/node": "^25.0.3",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "4.22.1",
    "googleapis": "^159.0.0",
    "helmet": "^7.1.0",
    "imap": "^0.8.19",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^9.0.3",
    "mailparser": "^3.9.1",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "typescript": "^5.9.3",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.6.2"
  },
  "packageManager": "npm@10.9.0",
  "private": true,
  "type": "module"
}
